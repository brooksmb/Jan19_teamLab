<h3>Create A Car!</h3>
<fieldset>
  <form method="POST" action="/cars">
    <div class="form-group col-md-3">
      <input name="title" class="form-control" placeholder="Make"/>
    </div>

    <div class="form-group col-md-3">
      <input name="content" class="form-control" placeholder="Model"/>
    </div>

    <div class="form-group col-md-3">
      <input type= "submit" class="form-control" placeholder="Submit"/>
    </div>


  </form>
</fieldset>